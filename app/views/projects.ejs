<% layout('layout') -%>

<%- partial('partials/search-panel') %>

<div class="row project-list-title">
    <div class="col-lg-12">
        <div class="col-lg-1"></div>
        <div class="col-lg-3">Project</div>
        <div class="col-lg-2">Key</div>
        <div class="col-lg-2">Type</div>
        <div class="col-lg-3">Project Lead</div>
        <div class="col-lg-1">Control</div>
    </div>
</div>

<% projects.forEach(function(project) { %>
    <div class="row">
        <div class="col-lg-12">
            <div class="project">
                <div class="row">
                    <div class="col-lg-1"><img src="/img/projectavatar.png"></div>
                    <div class="col-lg-3"><span><%= project.title %></span></div>
                    <div class="col-lg-2"><span><%= project.key %></span></div>
                    <div class="col-lg-2">
                        <img src="/img/service_project_type_icon.png">
                        <span><%= project.type %></span>
                    </div>
                    <div class="col-lg-3"><span><%= project.lead %></span></div>
                    <div class="col-lg-1">
                        <span class="glyphicon glyphicon-edit"></span>
                        <span class="glyphicon glyphicon-remove"></span>
                    </div>
                </div>
            </div>
        </div>
    </div>
<% }); %>
