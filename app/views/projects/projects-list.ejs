<% projects.forEach(function(project) { %>
    <div class="row">
        <div class="col-lg-12">
            <div class="project">
                <div class="row">
                    <div class="col-lg-1"><img src="/img/projectavatar.png"></div>
                    <div class="col-lg-3"><span><%= project.name %></span></div>
                    <div class="col-lg-2"><span><%= project.key %></span></div>
                    <div class="col-lg-2">
                        <img src="/img/service_project_type_icon.png">
                        <span><%= project.type %></span>
                    </div>
                    <div class="col-lg-3"><span><%= project.lead.fullName %></span></div>
                    <div class="col-lg-1">
                        <span class="glyphicon glyphicon-edit"></span>
                        <span class="glyphicon glyphicon-remove"></span>
                    </div>
                </div>
            </div>
        </div>
    </div>
<% }); %>