<div class="well search-well">
    <div class="form-group search-group">
        <input type="text" class="search-input" placeholder="Enter title or issue-key">
        <span class="glyphicon glyphicon-search search-icon"></span>
        <button type="button" class="btn btn-primary pull-right" data-toggle="modal" data-target="#createProjectModal">
            <span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span>
            <span>Create New Project</span>
        </button>
    </div>
    <div class="btn-group filter-btn" role="group">
        <button type="button" class="btn btn-default btn-primary">All</button>
        <button type="button" class="btn btn-default btn-primary">Completed</button>
        <button type="button" class="btn btn-default btn-primary">Uncompleted</button>
    </div>
    <div class="modal fade" id="createProjectModal" tabindex="-1" role="dialog" aria-labelledby="modalLabel"></div>
</div>